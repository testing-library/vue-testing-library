    "isomorphic-unfetch": "^4.0.2",
    "jest-environment-jsdom": "^29.5.0",
    "jest-serializer-vue": "^3.1.0",
    "kcd-scripts": "^13.0.0",
    "lodash.merge": "^4.6.2",
    "msw": "^1.1.0",
    "tsd": "^0.27.0",
    "typescript": "^4.9.5",
    "vee-validate": "^4.7.4",
    "vue": "^3.2.47",
    "vue-eslint-parser": "^9.1.0",
