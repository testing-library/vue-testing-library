  "description": "Simple and complete Vue DOM testing utilities that encourage good testing practices.",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "format": "kcd-scripts format",
    "build": "kcd-scripts build",
